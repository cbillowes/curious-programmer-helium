  "main": "n/a",
  "scripts": {
    "clean": "rm -rf public && rm -rf .cache",
    "clean:hardcore": "rm -rf node-modules && rm -f packages.json.lock && rm -rf public && rm -rf .cache",
    "build": "./build.sh",
    "develop": "npm run clean && gatsby develop --verbose --port 8000 --host localhost",
    "serve": "npm run clean && npm run build && gatsby serve",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "spell-check": "mdspell -n -a --en-us content/posts/**/*.md",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "test:watch": "jest --watchAll -u",
    "test": "jest -u"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
